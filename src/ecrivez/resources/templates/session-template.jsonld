{
    "@context": "https://schema.org/",
    "@type": "Session",
    "session": {
        "@type": "UUID",
        "uuid": "<session_id>"
    },
    "model": [{
        "@type": "Model",
        "name": "llama3.1",
        "fullName": "Ollama Llama 3.1 8B Instruct",
        "description": "The latest and most powerful model from Ollama",
        "provider": {
            "@type": "LocalInference",
            "name": "Ollama",
            "baseUrl": "http://localhost:11434"
        },
        "settings": {
            "temperature": 0.5,
            "top_p": 1,
            "max_tokens": 1000,
            "stop": [
                "\n\n"
            ]
        },
        "message_ids": []
    }],
    "messages": [
        {
            "@type": "Message",
            "role": "user",
            "content": "Hello, how are you?"
        }
    ]
}